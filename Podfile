pod 'AFNetworking', '~> 3.0.4'
pod 'SDWebImage', '~> 3.7.6'
#pod 'iOS-UMengMessage', '~> 1.3.0’
pod 'Masonry', '~> 0.6.4'
pod 'Toast', '~> 3.0'
pod 'IQKeyboardManager', '~> 3.3.4'
pod 'UIImageView-PlayGIF', '~> 1.0.5'
pod 'MJRefresh', '~> 3.1.7'
pod 'JxbPickTool', '~> 1.0'
pod 'INTULocationManager', '~> 4.2.0'
pod 'JZLocationConverter', '~> 1.0.0'
pod 'SDCycleScrollView', '~> 1.65'
pod 'HYBNetworking', '~> 3.2.2’
pod 'UIImage-Resize', '~> 1.0.1'
pod 'FormatterKit', '~> 1.8.1'
pod 'XHLaunchAd', '~> 2.1.3'
pod 'Fabric'
pod 'OHHTTPStubs'
pod 'UMengAnalytics-NO-IDFA'
pod 'MMDrawerController'
# 主模块(必须)
pod 'ShareSDK3', '~> 3.3.2'
# Mob 公共库(必须) 如果同时集成SMSSDK iOS2.0:可看此注意事项：http://bbs.mob.com/thread-20051-1-1.html

# UI模块(非必须，需要用到ShareSDK提供的分享菜单栏和分享编辑页面需要以下1行)
pod 'ShareSDK3/ShareSDKUI'

pod 'MOBFoundation', '~> 2.0.1'

# 平台SDK模块(对照一下平台，需要的加上。如果只需要QQ、微信、新浪微博，只需要以下3行)
pod 'ShareSDK3/ShareSDKPlatforms/QQ'
pod 'ShareSDK3/ShareSDKPlatforms/SinaWeibo'
pod 'ShareSDK3/ShareSDKPlatforms/WeChat'


pod 'JxbDebugTool', '~> 4.5', :configurations => ['Debug']
pod 'Reveal-iOS-SDK', :configurations => ['Debug']

end
